{
  "name": "simple-twitter",
  "version": "1.0.0",
  "description": "A simple twitter clone",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix \"./client\"",
